{
    "root": true,
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": ["airbnb-base", "prettier"],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "no-console": "error",
        "import/extensions": [
            {
              "ignorePackage":false
            }
          ]
    }
}
